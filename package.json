{
  "name": "jdenticon",
  "version": "2.2.0",
  "description": "Javascript identicon generator",
  "main": "dist/jdenticon-node.js",
  "exports": {
    "require": "./dist/jdenticon-node.js",
    "default": "./dist/jdenticon-node.mjs"
  },
  "browser": "dist/jdenticon-module",
  "jsdelivr": "dist/jdenticon.min.js",
  "unpkg": "dist/jdenticon.min.js",
  "types": "types/module.d.ts",
  "type": "module",
  "bin": {
    "jdenticon": "bin/jdenticon.js"
  },
  "dependencies": {
    "@types/node": "*",
    "canvas-renderer": "~2.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@types/jquery": "^3.3.30",
    "buble": "^0.20.0",
    "del": "^1.2.1",
    "eslint": "^7.4.0",
    "google-closure-compiler": "^20190618.0.0",
    "gulp": "^4.0.2",
    "gulp-buble": "^0.9.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace-with-sourcemaps": "^0.7.1",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-wrap": "^0.15.0",
    "gulp-zip": "^5.0.0",
    "multipipe": "^4.0.0",
    "rollup": "^2.21.0",
    "rollup-plugin-strip-banner": "^2.0.0",
    "tap": "^10.7.0",
    "typescript": "^3.5.3"
  },
  "scripts": {
    "build": "gulp build",
    "release": "gulp release",
    "lint": "eslint ./src/**",
    "test": "npm run lint && npm run test:unit && npm run test:types",
    "test:unit": "gulp build-tests && tap obj/test/node/*.js",
    "test:types": "tsc -p ./test/types/module/tsconfig.json & tsc -p ./test/types/umd/tsconfig.json"
  },
  "files": [
    "bin/",
    "dist/",
    "standalone/",
    "types/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dmester/jdenticon"
  },
  "bugs": {
    "url": "https://github.com/dmester/jdenticon/issues"
  },
  "keywords": [
    "javascript",
    "identicon",
    "jdenticon",
    "avatar"
  ],
  "author": "Daniel Mester Pirttij√§rvi",
  "license": "MIT",
  "homepage": "https://jdenticon.com/"
}
